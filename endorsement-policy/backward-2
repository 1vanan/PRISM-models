dtmc

module die

	// local state
	s : [0..15] init 0;

	[] s=0 -> 0.07 : (s'=1) + 0.93 : (s'=2);

	[] s=1 -> 0.01 : (s'=1) + 0.99 : (s'=4);
	[] s=2 -> 0.01 : (s'=2) + 0.99 : (s'=6);
	[] s=4-> 0.02 : (s'=9) + 0.98 : (s'=10);
	[] s=5-> 0.02 : (s'=5) + 0.98 : (s'=12);
	[] s=6-> 0.02 : (s'=13) + 0.98 : (s'=14);


	[] s=9-> (s'=15);

	[] s=15-> (s'=15);
	[] s=10-> (s'=15);
	[] s=13-> (s'=15);
	[] s=14-> (s'=15);
	[] s=12-> (s'=15);
endmodule

rewards
    true : 1;
endrewards